<launch>

    <!-- Running map server -->
    <arg name="map_file" default="$(find navigation)/maps_generated/map.yaml"/>
    <node name="map_server" pkg="map_server" type="map_server" args="$(arg map_file)" />

  	<!--- Run optimization test node -->
   	<node pkg="teb_local_planner" type="path_planner_node" name="path_planner" output="screen" />

	<!-- RVIZ -->

	<param name="robot_description" command="$(find xacro)/xacro '$(find ackermann_vehicle_description)/robot/urdf/Ackermann_robot.urdf.xacro'"/>

	<!--
  	<node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher"/>
	-->
	
  	<node pkg="rviz" type="rviz" name="rviz" args="-d $(find teb_local_planner)/cfg/rviz_test_optim.rviz" />

</launch>